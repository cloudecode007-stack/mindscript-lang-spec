      "goals": ["—Ü–µ–ª—å 1", "—Ü–µ–ª—å 2"]
    },
    "parameters": {
      "detail_level": "–∫—Ä–∞—Ç–∫–∏–π|—Å—Ä–µ–¥–Ω–∏–π|–ø–æ–¥—Ä–æ–±–Ω—ã–π",
      "format": "—Ç–µ–∫—Å—Ç|—Å–ø–∏—Å–æ–∫|—Ç–∞–±–ª–∏—Ü–∞|json|–∫–æ–¥",
      "tone": "—Ñ–æ—Ä–º–∞–ª—å–Ω—ã–π|–Ω–µ—Ñ–æ—Ä–º–∞–ª—å–Ω—ã–π|—Ç–µ—Ö–Ω–∏—á–µ—Å–∫–∏–π",
      "temperature": 0.7,
      "max_tokens": 1000,
      "creativity": 0.5
    },
