LOAD_VAR "–∏–º—è"
LOAD_CONST 1          # "!"
BUILD_STRING 3
STORE_VAR "—Å–æ–æ–±—â–µ–Ω–∏–µ"
LOAD_VAR "—Å–æ–æ–±—â–µ–Ω–∏–µ"
RETURN

LOAD_CONST 2          # "–ú–∏—Ä"
CALL 1                # –í—ã–∑–æ–≤ —Å 1 –∞—Ä–≥—É–º–µ–Ω—Ç–æ–º
STORE_VAR "—Ä–µ–∑—É–ª—å—Ç–∞—Ç"
